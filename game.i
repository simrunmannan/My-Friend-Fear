enum {eye1, eye2, eye3, eye4, eye5, eye6, playerRight, playerRightAttack, playerLeft, playerLeftAttack, enemy1, enemy2};

int timer1;
int eyeClose;
int vOff;
int hOff;


void hideSprites() {
    for (int i = 0; i < 128; i++) {
        shadowOAM[i].attr0 = (2 << 8);
    }
}

void init() {
    int counterRow = 0;
    int counterCol = 0;
    for (int i = 0; i < 9; i++) {
        eyes[i].row = 73 + (counterRow * 32);
        eyes[i].col = 36 + (68 * counterCol);
        eyes[i].height = 32;
        eyes[i].width = 32;
        eyes[i].active = 1;
        if (i == 2 || i == 5) {
            counterRow ++;
            counterCol = 0;
        } else {
            counterCol++;
        }
    }
    player.active = 1;
    player.row = 8;
    player.col = 0;
    player.height = 64;
    player.width = 32;
    player.right = 1;
    player.rAttack = 0;
    player.left = 0;
    player.lAttack = 0;
    enemy.height = 64;
    enemy.width = 32;
    enemy.row = 7;
    enemy.col = 200;
    enemy.active = 1;
    enemy.right = 0;
    enemy.left = 1;
    enemy.slowDown = 0;
    *(volatile unsigned short *)0x04000010 = hOff;
    *(volatile unsigned short *)0x04000012 = vOff;
    hOff = 0;
    vOff = 20;
    eyeClose = -1;

}

void draw() {
    for (int i= 0; i < 9; i++) {
        if (eyes[i].active) {
            shadowOAM[i].attr0 = eyes[i].row;
            shadowOAM[i].attr1 = eyes[i].col | (2 << 14);
            shadowOAM[i].attr2 = (0)*32+((eyes[i].type)*4);
        } else {
            shadowOAM[i].attr0 = eyes[i].row;
            shadowOAM[i].attr1 = eyes[i].col | (2 << 14);
            shadowOAM[i].attr2 = (0)*32+(20);
        }
    }
        if (player.right && !player.rAttack) {
            shadowOAM[10].attr0 = (2 << 8);
            shadowOAM[11].attr0 = (2 << 8);
            shadowOAM[12].attr0 = (2 << 8);
            shadowOAM[9].attr0 = player.row;
            shadowOAM[9].attr1 = player.col | (3 << 14);
            shadowOAM[9].attr2 = (4)*32+(0);
        } else if (player.right && player.rAttack) {
            shadowOAM[9].attr0 = (2 << 8);
            shadowOAM[11].attr0 = (2 << 8);
            shadowOAM[12].attr0 = (2 << 8);
            shadowOAM[10].attr0 = player.row;
            shadowOAM[10].attr1 = player.col | (3 << 14);
            shadowOAM[10].attr2 = (4)*32+(8);
        } else if (player.left && !player.lAttack) {
            shadowOAM[9].attr0 = (2 << 8);
            shadowOAM[10].attr0 = (2 << 8);
            shadowOAM[12].attr0 = (2 << 8);
            shadowOAM[11].attr0 = player.row;
            shadowOAM[11].attr1 = player.col | (3 << 14);
            shadowOAM[11].attr2 = (4)*32+(16);
        } else if (player.left && player.lAttack) {
            shadowOAM[9].attr0 = (2 << 8);
            shadowOAM[10].attr0 = (2 << 8);
            shadowOAM[11].attr0 = (2 << 8);
            shadowOAM[12].attr0 = player.row;
            shadowOAM[12].attr1 = player.col | (3 << 14);
            shadowOAM[12].attr2 = (4)*32+(24);
        }
        if (enemy.left) {
            shadowOAM[14].attr0 = (2 << 8);
            shadowOAM[13].attr0 = (0xFF & enemy.row);
            shadowOAM[13].attr1 = (0x1FF & enemy.col) | (3 << 14);
            shadowOAM[13].attr2 = (12)*32+(0);
            }
        if (enemy.right) {
            shadowOAM[13].attr0 = (2 << 8);
            shadowOAM[14].attr0 = (0xFF & enemy.row);
            shadowOAM[14].attr1 = (0x1FF & enemy.col) | (3 << 14);
            shadowOAM[14].attr2 = (12)*32+(8);
        }
        if (enemy.active == 0) {
            shadowOAM[13].attr0 = (2 << 8);
            shadowOAM[14].attr0 = (2 << 8);
        }
    if (game2) {
        hideSprites();
        DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128*4);
    }
    DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128*4);
}

void update() {
    for (int i = 0; i < 9; i++) {
        if (player.col <= 48 && eyes[i].active) {
            eyes[i].type = eye1;
        } else if (player.col > 48 && player.col <= 96 && eyes[i].active) {
             eyes[i].type = eye2;
        } else if (player.col > 96 && player.col <= 144 && eyes[i].active) {
             eyes[i].type = eye3;
        } else if (player.col > 144 && player.col <= 192 && eyes[i].active) {
             eyes[i].type = eye4;
        } else if (player.col > 192 && player.col <= 240 && eyes[i].active) {
             eyes[i].type = eye5;;
        }
    }
    if ((~(*(volatile unsigned int *)0x04000130) & ((1<<5))) && player.col > 0) {
        player.left = 1;
        player.right = 0;
        player.rAttack = 0;
        player.lAttack = 0;
        player.col = player.col - 1;
    }
    if ((~(*(volatile unsigned int *)0x04000130) & ((1<<4))) && player.col <= 260) {
        player.right = 1;
        player.left = 0;
        player.lAttack = 0;
        player.rAttack = 0;
        player.col = player.col + 1;
    }
    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6))))) {
        player.width = 34;
        playSoundTwo(Umbrella, 4200, 11025, 0, 0);
        if (player.right) {
            player.rAttack = 1;
            player.lAttack = 0;
        } else if (player.left) {
            player.lAttack = 1;
            player.rAttack = 0;
        }
    } else {
        player.width = 40;
    }
     if((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
        enemy.slowDown = 1;
    }
    int collide = collisionplayerenemy(&enemy, &player);

    if (enemy.left) {
        if (enemy.col > -20) {
            if (enemy.slowDown) {
                enemy.col -= 1;
            } else {
                enemy.col-= 2;
            }
        } else {
            if (rand()%2==0) {
                respawnRight(&enemy);
            } else {
                respawnLeft(&enemy);
            }
        }
        if (collide == 1) {
            enemy.slowDown = 0;
            eyeClose++;
            playSoundTwo(Blink,30466, 11025, 0, 0);
            eyes[eyeClose].active = 0;
            enemy.right = 1;
            enemy.left = 0;
            enemy.col += 15;

        }
        if (collide == 3) {
            stopSound();
            goToLose();
        }
     }
     else if (enemy.right) {
        if (enemy.col < 240) {
               if (enemy.slowDown) {
                enemy.col += 1;
            } else {
                enemy.col+= 2;
            }
        } else {
            if (rand()%2==0) {
                respawnRight(&enemy);
            } else {
                respawnLeft(&enemy);
            }
        }
         if (collide == 2) {
            enemy.slowDown = 0;
            eyeClose++;
            playSoundTwo(Blink,30466, 11025, 0, 0);
            eyes[eyeClose].active = 0;
            enemy.left = 1;
            enemy.right = 0;
            enemy.col -= 15;

        }
         if (collide == 3) {
            stopSound();
            goToLose();
        }
    }

        if (eyeClose == 9) {
            stopSound();
            enemy.active = 0;
            if (player.col >= 240) {
                game2 = 1;
                goToInstruct();
            }
        }
}

void initRain() {
    hOff = 0;
    vOff = 100;
    *(volatile unsigned short *)0x04000010 = hOff;
    *(volatile unsigned short *)0x04000012 = vOff;
}

void updateRain() {
    *(volatile unsigned short *)0x04000010 = hOff++;
    *(volatile unsigned short *)0x04000012 = vOff++;
}

int collisionplayerenemy(ENEMY*e, PLAYER*p) {
    if (e->active) {
        if (e->col <= p->col + p->width - 10 && e->col + e->width >= p->col) {
            if (p->rAttack && e->left) {
                return 1;
            }
            if (p->lAttack && e->right) {
                return 2;
            }
            if (p->left || p->right) {
                return 3;
            }
        }
    }
    return 0;
}

void respawnLeft(ENEMY*e) {
    e->col = -20;
    e->left = 0;
    e->right = 1;
}

void respawnRight(ENEMY*e) {
    e->col = 245;
    e->left = 1;
    e->right = 0;
}
